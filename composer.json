{
    "name": "konfig/snaptrade-php-sdk",
    "description": "Connect brokerage accounts to your app for live positions and trading",
    "keywords": [
        "openapi",
        "php",
        "sdk",
        "rest",
        "konfig",
        "api"
    ],
    "license": "unlicense",
    "authors": [
        {
            "name": "Konfig",
            "homepage": "https://konfigthis.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "SnapTrade\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "SnapTrade\\Test\\" : "test/" }
    }
}
